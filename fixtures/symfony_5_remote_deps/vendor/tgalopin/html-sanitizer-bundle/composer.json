{
    "name": "tgalopin/html-sanitizer-bundle",
    "description": "Symfony Bundle for https://github.com/tgalopin/html-sanitizer",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com"
        }
